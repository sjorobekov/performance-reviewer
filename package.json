{
  "name": "performance-reviewer",
  "version": "0.0.1",
  "adonis-version": "4.1.0",
  "description": "Performance Reviewer",
  "main": "server.js",
  "scripts": {
    "prestart": "npm run migration:up:force",
    "start": "cross-env NODE_ENV=production adonis serve",
    "start:dev": "run-p dev:*",
    "dev:back": "adonis serve --dev",
    "dev:front": "webpack --watch",
    "build:dev": "webpack",
    "build": "cross-env NODE_ENV=production webpack",
    "lint": "cross-env NODE_ENV=production eslint --ext .js,.vue ./",
    "lint:fix": "eslint --ext .js,.vue ./ --fix",
    "test": "node ace test",
    "migration:up": "adonis migration:run",
    "migration:up:force": "adonis migration:run --force",
    "migration:down": "adonis migration:rollback",
    "seed": "adonis seed --force"
  },
  "author": "Samat Jorobekov <samat.jorobekov@gmail.com",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cli": "^4.0.11",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/redis": "^2.0.7",
    "@adonisjs/session": "^1.0.28",
    "@adonisjs/validator": "^5.0.6",
    "axios": "^0.21.1",
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "pg": "^7.9.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.4.0",
    "@nuxt/babel-preset-app": "^2.5.1",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "node-notifier": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "vee-validate": "^2.2.2",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-moment": "^4.0.0",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.10",
    "vuetify": "^1.5.7",
    "vuex": "^3.1.0",
    "webpack": "^3.12.0",
    "webpack-notifier": "^1.5.0"
  },
  "autoload": {
    "App": "./app"
  },
  "engines": {
    "node": ">= 8.11.0",
    "npm": ">= 6.0.0"
